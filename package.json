{
  "name": "bola-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:unit": "npm run test:unit:cmd",
    "build:dev": "npm run pre-build && vue-cli-service build --mode dev",
    "build:nightly": "npm run pre-build && vue-cli-service build --mode nightly",
    "build:prod": "npm run pre-build && vue-cli-service build --mode prod",
    "build:stage": "npm run pre-build && vue-cli-service build --mode stage",
    "pre-build": "rm -rf dist",
    "serve:dev": "vue-cli-service serve --mode dev",
    "serve:nightly": "vue-cli-service serve --mode nightly",
    "serve:prod": "vue-cli-service serve --mode prod",
    "serve:stage": "vue-cli-service serve --mode stage",
    "lint": "eslint 'src/**/*.{ts,vue}' --config .eslintrc.js --fix",
    "test": "tsc --noEmit && npm run test:unit",
    "test:unit:cmd": "node --max_old_space_size=2048 ./node_modules/.bin/vue-cli-service test:unit --silent --logHeapUsage"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
